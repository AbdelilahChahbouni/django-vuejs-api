g : false,\n      checkedNames : ref([])\n      \n     \n\n    }\n  },\n\n  mounted(){\n    this.getCourses(\"\");\n  },\n\n  methods: {\n    getStr(myDic){\n      var path = '';\n      for (var key in myDic) {\n            \n          console.log(\"Key: \" + key + \", Value: \" + myDic[key]);\n          if (key == 0) {\n            if (myDic[key] == 10 || myDic[key]==20 || myDic[key]==30) {\n              path += \"price=\"+myDic[key]\n            }\n            else {\n              path += \"categorie=\"+myDic[key]\n            }\n            \n\n          }\n          else {\n            if (myDic[key] == 10 || myDic[key]==20 || myDic[key]==30) {\n              path += \"&price=\"+myDic[key]\n            }\n            else {\n              path += \"&categorie=\"+myDic[key]\n            }\n            \n          }\n          \n          console.log(myDic[key])\n          }\n      console.log(path)\n      return path\n        },\n\n    getCourses(s){\n      axios({\n        method:\"get\",\n        url:'http://127.0.0.1:8000/course/api/list?'+s,\n      }).then(response => this.course = response.data);\n           \n    },\n\n    searchFlag(){\n      this.flag=true;\n      this.getCourses('search='+this.data_input)\n    },\n\n  }\n}\n</script>\n\n\n\n\n\n\n\n\n\n\n<style>\nul{\n  list-style: none;\n}\n\nul li label{\n  margin-left: 5px;\n  \n\n\n}\n.my-logo {\n  width: 70px;\n  height: 70px;\n}\n.sp1 , .sp2{\n  margin-top: 5px;\n  margin-right: 5px;\n  margin-left: 10px;\n  font-size: 12px;\n  font-weight: 400;\n}\n.fa-cart-shopping {\n  margin-top:5px;\n  margin-left: 5px;\n  margin-right: 10px;\n}\n.my-btn {\n  margin-right: 5px;\n  border: 1px solid black;\n  font-size: 12px;\n  padding: 5px 5px;\n  width: 80px;\n  height: 30px;\n  \n}\n\n.fa-globe {\n  margin-left: 10px;\n  border: 1px solid black;\n  padding: 5px 5px;\n  height: 30px;\n\n}\n\n.my-btn2 {\n  margin-left: 5px;\n  background-color: black;\n  color: white;\n}\n.my-btn1 {\n  margin-left: 10px;\n}\n.my-input[placeholder]{\n  font-size: 12px;\n  font-family: 400;\n}\n\n.my-drop li span  {\n  margin-left: 10px;\n  \n}\n\na {\n  text-decoration: none;\n}\n\n.my-card {\n  display: flex;\n  border: 1px solid #eee;\n  margin-bottom: 20px;\n  border-radius: 4px;\n\n}\n.my-card .img-card {\n  width: 30%;\n}\n\n.my-card .img-card img {\n  width: 300px;\n  height: 250px;\n}\n\n.my-card .body-card{\n  width: 60%;\n  padding-top: 10px;\n  padding-bottom: 10px;\n}\n</style>\n\n\n"],"mappings":"AA0GA,SAASA,GAAE,QAAS,KAAI;AAExB,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,GAAG,EAAEC;EACP,CAAC;EAEDC,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLJ,IAAG,EAAI,WAAW;MAClBK,MAAK,EAAI,EAAE;MACXC,UAAS,EAAI,EAAE;MACfC,IAAG,EAAI,KAAK;MACZC,YAAW,EAAIV,GAAG,CAAC,EAAE;IAIvB;EACF,CAAC;EAEDW,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,UAAU,CAAC,EAAE,CAAC;EACrB,CAAC;EAEDC,OAAO,EAAE;IACPC,MAAMA,CAACC,KAAK,EAAC;MACX,IAAIC,IAAG,GAAI,EAAE;MACb,KAAK,IAAIC,GAAE,IAAKF,KAAK,EAAE;QAEnBG,OAAO,CAACC,GAAG,CAAC,OAAM,GAAIF,GAAE,GAAI,WAAU,GAAIF,KAAK,CAACE,GAAG,CAAC,CAAC;QACrD,IAAIA,GAAE,IAAK,CAAC,EAAE;UACZ,IAAIF,KAAK,CAACE,GAAG,KAAK,EAAC,IAAKF,KAAK,CAACE,GAAG,CAAC,IAAE,EAAC,IAAKF,KAAK,CAACE,GAAG,CAAC,IAAE,EAAE,EAAE;YACxDD,IAAG,IAAK,QAAQ,GAACD,KAAK,CAACE,GAAG;UAC5B,OACK;YACHD,IAAG,IAAK,YAAY,GAACD,KAAK,CAACE,GAAG;UAChC;QAGF,OACK;UACH,IAAIF,KAAK,CAACE,GAAG,KAAK,EAAC,IAAKF,KAAK,CAACE,GAAG,CAAC,IAAE,EAAC,IAAKF,KAAK,CAACE,GAAG,CAAC,IAAE,EAAE,EAAE;YACxDD,IAAG,IAAK,SAAS,GAACD,KAAK,CAACE,GAAG;UAC7B,OACK;YACHD,IAAG,IAAK,aAAa,GAACD,KAAK,CAACE,GAAG;UACjC;QAEF;QAEAC,OAAO,CAACC,GAAG,CAACJ,KAAK,CAACE,GAAG,CAAC;MACtB;MACJC,OAAO,CAACC,GAAG,CAACH,IAAI;MAChB,OAAOA,IAAG;IACR,CAAC;IAELJ,UAAUA,CAACQ,CAAC,EAAC;MACXnB,KAAK,CAAC;QACJoB,MAAM,EAAC,KAAK;QACZC,GAAG,EAAC,wCAAwC,GAACF;MAC/C,CAAC,CAAC,CAACG,IAAI,CAACC,QAAO,IAAK,IAAI,CAACjB,MAAK,GAAIiB,QAAQ,CAAClB,IAAI,CAAC;IAElD,CAAC;IAEDmB,UAAUA,CAAA,EAAE;MACV,IAAI,CAAChB,IAAI,GAAC,IAAI;MACd,IAAI,CAACG,UAAU,CAAC,SAAS,GAAC,IAAI,CAACJ,UAAU;IAC3C;EAEF;AACF"},"metadata":{},"sourceType"